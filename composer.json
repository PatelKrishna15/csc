{
    "name": "nihirz/csc",
    "description": "This library is used for creating country, state, city seeder & published by the user which automatically creates tables, migrations & models. Users can easily use it in their projects.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Nihir Zala",
            "email": "zalanihir8866@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {},
    "autoload": {
        "psr-4": {
            "Nihirz\\csc\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nihirz\\csc\\CSCServiceProvider"
            ]
        }
    },
    "scripts": {
